{
  "name": "edi-reader",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --configuration dev",
    "start:qat": "ng serve --configuration qat",
    "start:stg": "ng serve --configuration stg",
    "start:prd": "ng serve --configuration prd",
    "build:dev": "ng build --configuration dev",
    "build:qat": "ng build --configuration qat",
    "build:stg": "ng build --configuration stg",
    "build:prd": "ng build --configuration prd",
    "watch:dev": "ng build --watch --configuration dev",
    "watch:qat": "ng build --watch --configuration qat",
    "watch:stg": "ng build --watch --configuration stg",
    "watch:prd": "ng build --watch --configuration prd",
    "test": "ng test --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.1",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/platform-browser": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@primeuix/themes": "^2.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@eslint/js": "^9.39.2",
    "@types/jasmine": "~6.0.0",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.1",
    "jasmine-core": "^6.0.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.53.1"
  }
}
